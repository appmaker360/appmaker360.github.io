"\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid': fullWidth}\">\n        <div class=\"mbr-row mbr-black\" mbr-class=\"{'mbr-jc-s': rowalign == 1, 'mbr-jc-c': rowalign == 2, 'mbr-jc-e': rowalign == 3}\">\n\n            <div class=\"image-block mbr-col-md-12 mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-3':contentWidth==1,\n                                 'mbr-col-lg-4':contentWidth==2,\n                                 'mbr-col-lg-6':contentWidth==3,\n                                 'mbr-col-lg-8':contentWidth==4,\n                                 'mbr-col-lg-9':contentWidth==5}\">\n                <div class=\"image-wrap\">\n                    <amp-img src=\"@PROJECT_PATH@/assets/images/mbr-1456x971.jpg\" layout=\"responsive\" width=\"728.7497425334706\" height=\"486\" alt=\"app maker software\" class=\"placeholder-loader\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mbr-1456x971.jpg\" alt=\"app maker software\" title=\"app maker software\">\n                    </amp-img>\n                </div>\n            </div>\n\n            <div class=\"title-wrap mbr-col-md-12 mbr-col-sm-12\" mbr-class=\"{'mbr-col-lg-3':contentWidth==1,\n                             'mbr-col-lg-4':contentWidth==2,\n                             'mbr-col-lg-6':contentWidth==3,\n                             'mbr-col-lg-8':contentWidth==4,\n                             'mbr-col-lg-9':contentWidth==5,\n                             'align-left':alignContent==1,\n                             'align-center':alignContent==2,\n                             'align-right':alignContent==3\n                             }\" mbr-if=\"showTitle || showSubtitle || showText || showButtons\">\n                <div class=\"title-block\">\n                    <h1 class=\"mbr-section-title mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .mbr-section-btn\">Mobile solutions made for businesses</h1>\n                    <h2 class=\"mbr-section-subtitle mbr-fonts-style mbr-black\" mbr-class=\"{'mbr-pt-2': showTitle}\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle, .mbr-section-btn\">Enter\n                        your subtitle here</h2>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-class=\"{'mbr-pt-2': showTitle || showSubtitle}\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-section-btn, .mbr-text\">Increase revenue, grow a loyal customer base, and strengthen your online presence with our mobile features. We're an app development team dedicated to your success.</p>\n                    <div mbr-buttons=\"\" class=\"mbr-section-btn\" mbr-class=\"{'mbr-pt-4':showSubtitle||showTitle||showText}\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove\" mbr-if=\"showButtons\"><a class=\"btn btn-black-outline\" data-app-placeholder=\"Type Text\">Learn More</a></div>\n                </div>\n            </div>\n\n\n\n\n\n        </div>\n    </div>\n</section>