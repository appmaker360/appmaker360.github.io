          "_customHTML": "<section class=\"form1\" group=\"Forms\" plugins=\"amp-main, amp-form, amp-mustache, amp-date-picker\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <input type=\"color\" title=\"Border Color\" name=\"border\" value=\"#cccccc\">\n        <input type=\"color\" title=\"Input BG Color\" name=\"inputBg\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Input Text Color\" name=\"inputText\" value=\"#bbbbbb\">\n        <input type=\"color\" title=\"Header Color\" name=\"headerColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Body Color\" name=\"bodyColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Header BG Color\" name=\"headerBg\" value=\"#767676\">\n        <input type=\"color\" title=\"Body BG Color\" name=\"bodyBg\" value=\"#767676\">\n        <input type=\"color\" title=\"Selected Day BG\" name=\"selectedBg\" value=\"#365c9a\">\n        <input type=\"color\" title=\"Selected Day Color\" name=\"selectedColor\" value=\"#ffffff\">\n        <input type=\"text\" title=\"Date Format\" name=\"dateFormat\" value=\"YYYY-MM-DD\">\n        <select title=\"Date Picker Locale\" name=\"datepickerLocale\">\n            <option value=\"en\" selected>English</option>\n            <option value=\"ru\">Russian</option>\n            <option value=\"de\">German</option>\n            <option value=\"es\">Spanich</option>\n            <option value=\"fr\">French</option>\n            <option value=\"it\">Italian</option>\n        </select>\n        <input type=\"text\" title=\"Min Date\" name=\"minDate\" value=\"2018-08-01\">\n        <input type=\"text\" title=\"Max Date\" name=\"maxDate\" value=\"2018-10-01\">\n\n        <input type=\"text\" title=\"Min Time\" name=\"minTime\" value=\"08:00\">\n        <input type=\"text\" title=\"Max Time\" name=\"maxTime\" value=\"18:00\">\n        <input type=\"text\" title=\"Time Step (min)\" name=\"timeStep\" value=\"15\">\n\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"bg.type!='color' && overlay\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container align-center mbr-white\">\n        <div class=\"title-wrap\" mbr-if=\"showTitle || showSubtitle\">\n            <h1 class=\"mbr-section-title mbr-black mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .input-group-btn, .underline\">Free Test Drive</h1>\n            <h2 class=\"mbr-section-subtitle mbr-black mbr-fonts-style mbr-light\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle, .input-group-btn\">Lorem ipsum dolor sit amet, consectetur\n                adipisicing elit.</h2>\n        </div>\n\n        <div class=\"mbr-row mbr-jc-c\">\n            <div mbr-form class=\"mbr-col-sm-12 mbr-col-md-10 mbr-col-lg-8\">\n                <form class=\"mbr-form\" action=\"support@appmaker360.com\" method=\"post\" name=\"8b Form\" target=\"_top\" novalidate>\n                    <div submit-success>\n                        <template data-form-alert type=\"amp-mustache\">Success! Check Your Email for Free Test Drive Information.</template>\n                    </div>\n                    <div submit-error>\n                        <template data-form-alert type=\"amp-mustache\">Success! Check Your Email for Free Test Drive Information.</template>\n                    </div>\n                    <div class=\"fieldset\">\n                        <div class=\"split-row\">\n                            <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Name\">\n                            <input type=\"text\" name=\"form[data][0][1]\" data-form-field=\"Name\" placeholder=\"Create a username\" required>\n\n                            <input type=\"hidden\" name=\"form[data][1][0]\" value=\"Company\">\n                            <input type=\"tel\" name=\"form[data][1][1]\" data-form-field=\"Company\" placeholder=\"Website address\" required>\n                        </div>\n\n                        <div class=\"split-row\">\n                            <input type=\"hidden\" name=\"form[data][6][0]\" value=\"Email\">\n                            <input type=\"text\" name=\"form[data][6][1]\" data-form-field=\"Email\" placeholder=\"Email address\" required>\n\n                            <input type=\"hidden\" name=\"form[data][7][0]\" value=\"Phone\">\n                            <input type=\"tel\" name=\"form[data][7][1]\" data-form-field=\"Phone\" placeholder=\"Phone number\" required hidden=\"hidden\">\n                        </div>\n\n\n                        <input type=\"hidden\" name=\"form[data][2][0]\" value=\"Interested\">\n                        <input type=\"email\" name=\"form[data][2][1]\" data-form-field=\"Interested\" placeholder=\"Interested in\" required 